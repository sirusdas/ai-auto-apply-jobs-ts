{
  "manifest_version": 3,
  "name": "LinkedIn Jobs AI Auto Apply",
  "version": "2.0.2",
  "description": "Automate & autofill job applications with AI",
  "icons": {
    "16": "laaa_logo_16x16.png",
    "48": "laaa_logo_48x48.png",
    "128": "laaa_logo_128x128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "notifications"
  ],
  "host_permissions": [
    "https://qerds.com/*",
    "https://*.linkedin.com/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; frame-src https://www.youtube.com https://www.youtube-nocookie.com"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Auto Apply Settings",
    "default_icon": {
      "16": "laaa_logo_16x16.png",
      "48": "laaa_logo_48x48.png",
      "128": "laaa_logo_128x128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.linkedin.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "options_ui": {
    "page": "settings.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "assets/compressed_resume.yaml"
      ],
      "matches": [
        "https://*.linkedin.com/*"
      ]
    }
  ]
}